</head>
<body>
   <%- include("../partials/navbar")%>
  <div class="card-container">
    <% for(let d of data) { %>
        <div class="card" >
            <a href="http://localhost:3000/book/<%=d._id%>">
            <img class="card-image" src="<%= d.image %>" alt="Image">
            <h3 class="card-title"><%= d.title %></h3>
            <h4 class="card-price">â‚¹ <%= d.price.toLocaleString("en-IN") %></h4>
            </a>
        </div>
    <% } %>
</div>
<%- include("../partials/footer")%>


</body>
</html>